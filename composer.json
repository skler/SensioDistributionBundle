{
    "name": "skler/distribution-bundle",
    "description": "The base bundle for the Symfony Distributions with multi-kernel support",
    "keywords": ["distribution","configuration", "multikernel"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Mauro Foti",
            "email": "m.foti@teckhouse.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.2"
    },
    "autoload": {
        "psr-0": { "Sensio\\Bundle\\DistributionBundle": "" }
    },
    "target-dir": "Sensio/Bundle/DistributionBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "minimum-stability": "stable"
}
